<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".customerhomepage.consumerpg12"
    android:background="@drawable/bg_login"
    >

    <TextView
        android:id="@+id/no_hours"
        android:layout_width="342dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:background="@color/white"
        android:text=""
        android:textColor="@color/white"
        android:textSize="26dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="308dp" />

    <TextView
        android:id="@+id/summary_txt"
        android:layout_width="346dp"
        android:layout_height="107dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="32dp"
        android:text="We would love to hear from your experience with our FIXXER today! "
        android:textAlignment="center"
        android:textColor="@color/white"

        android:textSize="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fixxer_id_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="156dp"
        android:text="FIXXER ID"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/worker_type_display"
        android:layout_width="342dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="196dp"
        android:background="@color/white"
        android:text=""
        android:textColor="@color/white"
        android:textSize="26dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fixxer_name"
        android:layout_width="293dp"
        android:layout_height="31dp"
        android:layout_marginStart="40dp"
        android:text="FIXXER Name"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="263dp" />

    <TextView
        android:id="@+id/amountPERhour"
        android:layout_width="340dp"
        android:layout_height="55dp"
        android:layout_marginStart="40dp"
        android:text="How would you rate (FIXXER's Name)'s work today?"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="373dp" />

    <TextView
        android:id="@+id/why_rate_text"
        android:layout_width="338dp"
        android:layout_height="52dp"
        android:layout_marginStart="44dp"
        android:text="Can you give the reasons why you gave this score?"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="493dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="66dp"
        android:layout_height="50dp"
        android:background=""
        app:srcCompat="@drawable/ic_thumb_up"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="435dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="58dp"
        android:layout_height="51dp"
        android:background=""
        app:srcCompat="@drawable/ic_thumb_down_24"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="434dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="310dp"
        android:layout_height="71dp"
        android:background="#50ffffff"
        android:text=""
        android:textSize="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="554dp" />

    <Button
        android:id="@+id/signupBtn"
        android:layout_width="151dp"
        android:layout_height="59dp"
        android:layout_marginStart="128dp"
        android:background="@drawable/bg_button"
        android:elevation="20dp"
        android:fontFamily="@font/roboto_bold"
        android:padding="10dp"
        android:text="Submit"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="644dp" />


</androidx.constraintlayout.widget.ConstraintLayout>