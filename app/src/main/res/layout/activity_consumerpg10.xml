<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".customerhomepage.consumerpg10"
    android:background="@drawable/bg_login"
    >

    <TextView
        android:id="@+id/txt_tips"
        android:layout_width="356dp"
        android:layout_height="64dp"
        android:text="Would you like to give some tips for our FIXXER today?"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20dp"

        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="46dp" />

    <TextView
        android:id="@+id/text_subtotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Subtotal"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="136dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/subtotal"
        android:layout_width="98dp"
        android:layout_height="37dp"
        android:text="$"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="30dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="173dp" />

    <Button
        android:id="@+id/button_tip15"
        android:layout_width="329dp"
        android:layout_height="39dp"
        android:background="#50ffffff"
        android:text="15% ($)"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="285dp" />

    <Button
        android:id="@+id/button_tip20"
        android:layout_width="329dp"
        android:layout_height="39dp"
        android:background="#50ffffff"
        android:text="20% ($)"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="346dp" />

    <Button
        android:id="@+id/button_tip30"
        android:layout_width="329dp"
        android:layout_height="39dp"
        android:background="#50ffffff"
        android:text="30% ($)"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="408dp" />

    <Button
        android:id="@+id/button_tip_custom"
        android:layout_width="329dp"
        android:layout_height="39dp"
        android:background="#50ffffff"
        android:text="custom"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="18dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="471dp" />

    <Button
        android:id="@+id/button_tip4"
        android:layout_width="329dp"
        android:layout_height="39dp"
        android:background="#50ffffff"
        android:text="No Tip"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="576dp" />
</androidx.constraintlayout.widget.ConstraintLayout>